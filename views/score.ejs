<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Bangpakok Hackathon Qualify</title>
    <link href="/css/bootstrap.min.css" rel="stylesheet" crossorigin="anonymous">
    <link rel="icon" type="image/x-icon" href="https://media.discordapp.net/attachments/1087795365027258398/1146300900311306300/logo.png?width=556&height=676">
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</head>
<body>
    <% if (type === true) { %>
        <script>
        Swal.fire({
            title: 'Congratulations!',
            text: "You've passed the testcase.",
            icon: 'success',
            showCancelButton: false,
            confirmButtonColor: '#3085d6',
            confirmButtonText: 'OK'
        }).then((result) => {
            if (result.isConfirmed) {
                window.location.href = '/problem';
            }
        });
    </script>
    <% } else if (type === false) { %>
        <script>
        Swal.fire({
            title: 'Oops!',
            text: 'You failed the testcase. Keep trying!',
            icon: 'error',
            showCancelButton: false,
            confirmButtonColor: '#3085d6',
            confirmButtonText: 'OK'
        }).then((result) => {
            if (result.isConfirmed) {
                window.location.href = '/problem';
            }
        });
    </script>
    <% } %>
</body>
</html>
